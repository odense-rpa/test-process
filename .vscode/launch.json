{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug main.py (without --queue)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug main.py (with --queue)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": ["--queue"],
            "console": "integratedTerminal"
        }
    ]
}
